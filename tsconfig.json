{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "ESNEXT",
    "module": "ESNext",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "plugins": [
      { 
        "name": "typescript-plugin-css-modules",
        "options": {

        }
      }
    ],
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "moduleResolution": "node",
    "rootDir": ".",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "importHelpers": true,
    "strictNullChecks": false,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true,
    "allowJs": true,
    "experimentalDecorators": true,
    "paths": {
      "@components/": [
        "./src/components/*"
      ],
      "@":[
        "src"
      ],
      "pages": [
        "src/pages"
      ],
      "@utils":[
        "src/utils/*"
      ],
      "@services":[
        "./src/services/*"
      ],
      "@models":[
        "./src/models/*"
      ],
    },
    "typeRoots": [
      "node_modules/@types",
      "typings"
    ]
  },
  "include": [
    "src",
  ],
  "exclude": [
    "node_modules",
  ]
}